<xsd:element name="getMailingAddressRequest">
     <xsd:complexType>
            <xsd:sequence>
           
            <xsd:element name="TypeofMailingadress" type="xsd:string" />
            <xsd:choice minOccurs="0">
                 <xsd:element name="USAddress" type="xsd:string" />
                 <xsd:element name="Military" type="xsd:string" />
                    <xsd:element name="ForeignAddress" type="tns:ForeignAddressType" minOccurs="0" />
                 <xsd:element name="U.S.Posssession" type="xsd:string" minOccurs="0" />
                 
                </xsd:choice>
                 <xsd:element name="Address" type="xsd:string" />
                 <xsd:element name="City" type="xsd:string"/>
                  <xsd:element name="state" type="tns:StateType"/>
                   <xsd:element name="ZipCode" type="xsd:string" />
                     <xsd:element name="phoneNo" type="xsd:int" /> 
                 <xsd:element name="makeMoneyotherthanUS" type="xsd:boolean" />           
               
       </xsd:sequence>
       </xsd:complexType>   
    </xsd:element>
<xsd:element name="getMailingAddressResponse">

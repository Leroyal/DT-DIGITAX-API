<xsd:element name="getSpouseTaxSubmissionRequest">
     <xsd:complexType>
            <xsd:sequence>
           
            <xsd:element name="SpouseFirstName" type="xsd:string" />
                 <xsd:element name="SpouseLastName" type="xsd:string" />
                 <xsd:element name="SpouseSocialSecurityNumber" type="tns:SSNType"/>
                  <xsd:element name="SpouseOccupation" type="xsd:string"/>
                   <xsd:element name="Date-of-birth" type="xsd:date" />
                     <xsd:element name="SpouseState" type="tns:StateType" /> 
                             
                <xsd:element name="FirstTaxFillingYear" type="xsd:date"/>
                <xsd:element name="Dependant" type="xsd:int"/>
                 <xsd:element name="DependantfirstName" type="xsd:string"/>
                 <xsd:element name="DependantLastName" type="xsd:string"/>
                   <xsd:element name="citizenship" type="xsd:string"/>
                         <xsd:element name="dependantDateofBirth" type="xsd:string"/>
                         <xsd:element name="Gender" type="xsd:string"/>
                        
                        <xsd:choice minOccurs="0">
                 <xsd:element name="disability" type="xsd:string" minOccurs="0"/>
                 <xsd:element name="passedaway" type="xsd:string" minOccurs="0"/>
                 <xsd:element name="notdependant" type="xsd:string" minOccurs="0" />
                                                 
                </xsd:choice>
                <xsd:choice minOccurs="0">
                 <xsd:element name="adopted" type="xsd:string" minOccurs="0"/>
                 <xsd:element name="foster" type="xsd:string" minOccurs="0"/>
                 
                                                 
                </xsd:choice>
                  <xsd:element name="durationInYear" type="xsd:string" />
       </xsd:sequence>
       </xsd:complexType>   
    </xsd:element>
